package view;
import processing.core.PApplet;
public class Cuadrado {
	 private PApplet app;
	 private int posX;
	 private int posY;
	 private int tipo;
	 
	public Cuadrado(int posX, int posY, int tipo, PApplet app) {
		
		this.app=app;
		
		
	}
	
	public void pintar(){
		
		app.rect(posx,posy,tamano,tamano);
		
	}
	
	
	
	
}

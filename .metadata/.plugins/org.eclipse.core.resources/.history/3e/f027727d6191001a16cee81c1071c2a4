package view;


import processing.core.PApplet;

public class Main extends PApplet {

	int [][] matriz;
	int tamañox;
	int tamañoy;
	

	public static void main(String[] args) {
		
		PApplet.main("view.Main");
	}
	
	
	public void settings() {
		size (600,400);
		
		
	}
	
	public void setup() {
		tamañox=30;
		tamañoy=20;
		
		for (int x=0; x < 20; x++) {
			  for (int y=0; y < 30; y++) {
			    if (y % 2 == 0) {
			    	
			    	matriz[x][y]=1;
			    }else {
			    	
			    	matriz[x][y]=0;
			    	
			    }
			  }
			}
		
		
	}
	
	public void draw() {
		
		background(150);
	}

}
